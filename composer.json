{
    "name": "andywer/js-localization",
    "description": "Laravel package to provide localizations to the Javascript logic.",
    "authors": [
        {
            "name": "Andy Wermke",
            "email": "andy@dev.next-step-software.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "illuminate/support": "4.0.x"
    },
    "require-dev": {
        "illuminate/foundation": "4.0.x",
        "mockery/mockery": "dev-master@dev",
        "phpunit/phpunit": "3.7.*"
    },
    "autoload": {
        "classmap": [
            "src/controllers",
            "tests/TestCase.php"
        ],
        "psr-0": {
            "JsLocalization": "src/"
        }
    },
    "extra": {
        "component": "package",
        "frameworks": ["Laravel 4"]
    },
    "minimum-stability": "dev"
}